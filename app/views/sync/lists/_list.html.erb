<li id="list-<%= list.id %>">
    <div class="col s6">
    
      <div class="card">
        <div class="card-content">
          <span class="card-title"><%= list.name %></span>
          <ul class="collection" >
            <%= render list.list_items %>
          </ul>
          <div class="card-action">
            <%= link_to 'View', list, class: "btn waves-effect waves-light" %>
           </div>
        </div>
    </div>
  </div>
</li>
